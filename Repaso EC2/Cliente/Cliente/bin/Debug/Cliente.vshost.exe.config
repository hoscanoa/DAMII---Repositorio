<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
              <binding name="WsAuthImplPortBinding">
                <security mode="TransportCredentialOnly">
                  <transport clientCredentialType="Basic" proxyCredentialType="None" realm="" />
                  <message clientCredentialType="UserName" algorithmSuite="Default" />
                </security>
              </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/ServicioWeb/WsAuthImplService"
                binding="basicHttpBinding" bindingConfiguration="WsAuthImplPortBinding"
                contract="SwServiceReference.WsAuth" name="WsAuthImplPort" />
        </client>
    </system.serviceModel>
</configuration>